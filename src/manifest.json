
{
  "manifest_version": 3,
  "name": "Tab Manager Google Chrome Extension",
  "version": "1.0",
  "options_page": "options.html",
  "icons": {
    "16": "images/tabs icon.png",
    "32": "images/tabs icon 2.png",
    "48": "images/tabs icon 3.png",
    "128": "images/tabs icon 4.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tabs": true
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }

}





